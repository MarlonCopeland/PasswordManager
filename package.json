{
  "name": "swiss",
  "version": "1.0.0",
  "description": "",
  "main": "src\\main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . swiss --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=Unjaded --version-string.FileDescription=Unjaded --version-string.ProductName=\"Swiss\"",
    "package-linux": "electron-packager . swiss --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/linux/icon.png --prune=true --out=release-builds"
  },
  "author": "unjaded",
  "license": "MIT",
  "dependencies": {
    "crypto-js": "^3.1.9-1",
    "electron": "^2.0.18",
    "electron-json-storage": "^4.1.6",
    "electron-packager": "^12.2.0",
    "electron-reload": "^1.4.0",
    "fs-extra": "^7.0.1",
    "gray-matter": "^4.0.2",
    "handlebars": "^4.1.2",
    "jquery": "^3.4.0",
    "tedious": "^3.0.1",
    "underscore": "^1.9.1"
  }
}
